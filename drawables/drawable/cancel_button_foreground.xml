<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
  <item>
    <shape android:shape="oval">
      <size
          android:height="30dp"
          android:width="30dp" />
      <solid android:color="@color/transparent" />
    </shape>
  </item>
  <item
      android:top="10dp"
      android:right="10dp"
      android:left="10dp"
      android:bottom="10dp">
    <rotate
        android:fromDegrees="45"
        android:toDegrees="45"
        android:pivotX="50%"
        android:pivotY="50%">
      <shape android:shape="line">
        <stroke
            android:color="@color/black"
            android:width="2dp" />

      </shape>
    </rotate>
  </item>
  <item
      android:top="10dp"
      android:right="10dp"
      android:left="10dp"
      android:bottom="10dp">
    <rotate
        android:fromDegrees="-45"
        android:toDegrees="-45"
        android:pivotX="50%"
        android:pivotY="50%">
      <shape android:shape="line">
        <stroke
            android:color="@color/black"
            android:width="2dp" />
      </shape>
    </rotate>
  </item>
</layer-list>